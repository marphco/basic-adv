{
  "routes": [
    {
      "src": "/portfolio/.*",
      "status": 410
    },
    {
      "src": "/login",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex"
        }
      ]
    },
    {
      "src": "/dashboard",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex"
        }
      ]
    },
    {
      "src": "/cookie-policy",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex"
        }
      ]
    },
    {
      "src": "/privacy-policy",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex"
        }
      ]
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}